    }
  },
  "AppSettings": {
    "HookApi": true
  },
  "ApiTest": {
    "HttpRouteAddress": "http://localhost:5000",
    "Station": "login",
    "Api": "api/login",
    "Argument": "{\"MobilePhone\":\"15618965007\",\"UserPassword\":\"123456A\",\"VerificationCode\":\"9999\"}"
  },
  "LogRecorder": {
    /*日志记录的地址
    "txtPath": "c:\\log\\auth",*/
    /*是否开启跟踪日志*/
    "monitor": true,
    /*是否开启SQL语句日志*/
    "sql": false,
    "level": "Debug",
    "console": true
  }
