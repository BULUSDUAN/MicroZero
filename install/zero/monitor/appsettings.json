{
  "urls": "http://*:5050",
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "AppName": "WebMonitor",
  "Zero": {
    "Global": {
      "ZeroGroup": [
        {
          "Name": "Dev",
          /*ZeroCenter主机IP地址*/
          "Address": "47.111.0.73",
          /*ZeroCenter管理端口号*/
          "ManagePort": 14000,
          /*ZeroCenter监测端口号*/
          "MonitorPort": 14001,
          /*服务器唯一标识*/
          "ServiceKey": "20180410"
        }
      ],
      /*ZeroApplication是否发出与ZeroCenter相关的同步事件,工具扩展应用使用*/
      "CanRaiseEvent": true,
      /*限速模式（Single 单线程 ThreadCount 按线程数限制 WaitCount 按等待数限制）*/
      "SpeedLimitModel": "WaitCount",
      /*最大等待数*/
      "MaxWait": 1000,
      "TaskCpuMultiple": 1
    },
    "WebMonitor": {
      "StationName": "WebMonitor",
      "ShortName": "wm"
    }
  },
  "ConnectionStrings": {
    /*数据库连接*/
    "ZeroTrace": "Database=db_zero_trace;Data Source=192.168.123.129;User Id=root;Password=123456;Persist Security Info=true;pooling=true;SslMode = none;CharSet=utf8;port=3306"
  },
  "WebSocket": {
    "Folders": [ "config", "status", "kline", "api", "plan_notify", "event", "trace_flow" ]
  },
  "LogRecorder": {
    "monitor": false,
    "sql": false,
    "level": "Warning",
    "console": true
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}