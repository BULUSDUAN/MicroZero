{
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "AppName": "ApiTest",
  "Weixin": null,
  "Zero": {
    "Global": {
      "ZeroGroup": [
        {
          "Name": "Dev",
          /*ZeroCenter主机IP地址*/
          "Address": "47.111.0.73",
          /*ZeroCenter管理端口号*/
          "ManagePort": 14000,
          /*ZeroCenter监测端口号*/
          "MonitorPort": 14001,
          /*服务器唯一标识*/
          "ServiceKey": "20180410"
        },
        {
          "Name": "Local",
          /*ZeroCenter主机IP地址*/
          "Address": "192.168.123.129",
          /*ZeroCenter监测端口号*/
          "MonitorPort": 8001,
          /*ZeroCenter管理端口号*/
          "ManagePort": 8000,
          /*服务器唯一标识*/
          "ServiceKey": "agebull"
        }
      ],
      /*限速模式（Single 单线程 ThreadCount 按线程数限制 WaiCtount 按等待数限制）*/
      "SpeedLimitModel": "WaitCount",
      /*最大等待数*/
      "MaxWait": 256,
      /*最大Task与Cpu核心数的倍数关系*/
      "TaskCpuMultiple": 1,
      /*API最大执行超时时间(单位秒)*/
      "ApiTimeout": 2,
      /*检测到API执行超时是否强行清除资源*/
      "ApiTimeoutKill": true
    },
    "SHIS": {
      "StationName": "ApiTest"
    },
    "socketOption": {
      "Backlog": 50000, //连接队列数量
      "ConnectTimeout": 500, // 连接失败时长
      "HeartbeatIvl": 0, //心跳间隔时长
      "HeartbeatTimeout": 200, //心跳超时时长
      "HeartbeatTtl": 200, //心跳TTL
      "Linger": 200, //自动关闭时长
      "ReconnectIvl": 10, // 重新连接时间间隔
      "ReconnectIvlMax": 500, //重新连接失败时长
      "RecvTimeout": 5000, //接收超时时长
      "SendTimeout": 5000, //发送超时时长
      "TcpKeepalive": 0, //启用Keeplive
      "TcpKeepaliveIdle": 4096, //TCP发送keepalive消息的频度,单位秒
      "TcpKeepaliveIntvl": 4096 //当TcpKeepalive探测没有确认时，重新发送探测的频度,单位秒
    }
  },
  "AppSettings": {
  },
  "LogRecorder": {
    /*日志记录的地址
    "txtPath": "c:\\log\\auth",*/
    /*是否开启跟踪日志*/
    "monitor": true,
    /*是否开启SQL语句日志*/
    "sql": false,
    "level": "Debug",
    "console": true
  }
}
