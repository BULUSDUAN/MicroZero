{
  "version": 2.1,
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "AppName": "RpcTest",
  "Zero": {
    "Global": {
      /*ZeroCenter主机IP地址*/
      "ZeroAddress": "zero.yizuanbao.cn",
      /*ZeroCenter监测端口号*/
      "ZeroMonitorPort": 12001,
      /*ZeroCenter管理端口号*/
      "ZeroManagePort": 12000,
      /*服务器唯一标识*/
      "ServiceKey": "20180410",
      "CanRaiseEvent": true
    },
    "RpcTest": {
      "StationName": "RpcTest",
      /*限速模式（Single 单线程 ThreadCount 按线程数限制 WaitCount 按等待数限制）*/
      "SpeedLimitModel": "ThreadCount",
      /*最大等待数*/
      "MaxWait": 1000,
      /*最大Task与Cpu核心数的倍数关系*/
      "TaskCpuMultiple": 2,
      "AddInPath": ""
    },
    "socketOption": {
      "Backlog": 50000, //连接队列数量
      "ConnectTimeout": 500, // 连接失败时长
      "Linger": 200, //自动关闭时长
      "ReconnectIvl": 50, // 重新连接时间间隔
      "ReconnectIvlMax": 500, //重新连接失败时长
      "RecvTimeout": 3000, //接收超时时长
      "SendTimeout": 3000, //发送超时时长
      "TcpKeepalive": 1, //启用Keeplive
      "TcpKeepaliveIdle": 5, //TCP发送keepalive消息的频度,单位秒
      "TcpKeepaliveIntvl": 5, //当TcpKeepalive探测没有确认时，重新发送探测的频度,单位秒
      "HeartbeatIvl": 0, //心跳间隔时长
      "HeartbeatTimeout": 0, //心跳超时时长
      "HeartbeatTtl": 0 //心跳TTL
    }
  },
  "AppSettings": {
    "HookApi": true
  },
  "ApiTest": {
    "HttpRouteAddress": "http://zero.yizuanbao.cn",
    "Station": "Authority",
    "Api": "v1/login/hpc",
    "Argument": "{\"MobilePhone\":\"15618965007\",\"UserPassword\":\"123456\"}"
  },
  "LogRecorder": {
    "console": true,
    "level": "Debug",
    /*是否开启跟踪日志*/
    "monitor": false,
    /*是否开启SQL语句日志*/
    "sql": false
  }
}