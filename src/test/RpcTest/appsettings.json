{
  "version": 2.1,
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "AppName": "RpcTest",
  "Zero": {
    "Global": {
      "ZeroAddress": "47.98.136.99",
      "ZeroManagePort": 14000,
      "ZeroMonitorPort": 14001,
      "ServiceKey": "u9RAEm5HpNOGK5wo",


      //"ZeroAddress": "192.168.123.129",
      //"ZeroMonitorPort": 8001,
      //"ZeroManagePort": 8000,
      //"ServiceKey": "agebull",

      /*限速模式（Single 单线程 ThreadCount 按线程数限制 WaitCount 按等待数限制）*/
      "SpeedLimitModel": "WaitCount",
      "MaxWait": 256,
      /*最大Task与Cpu核心数的倍数关系*/
      "TaskCpuMultiple": 2,
      "CanRaiseEvent": true
    },
    "RpcTest": {
      "StationName": "RpcTest"
    },
    "socketOption": {
      "Backlog": 50000, //连接队列数量
      "ConnectTimeout": 500, // 连接失败时长
      "Linger": 200, //自动关闭时长
      "ReconnectIvl": 50, // 重新连接时间间隔
      "ReconnectIvlMax": 500, //重新连接失败时长
      "RecvTimeout": 3000, //接收超时时长
      "SendTimeout": 3000, //发送超时时长
      "TcpKeepalive": 1, //启用Keeplive
      "TcpKeepaliveIdle": 5, //TCP发送keepalive消息的频度,单位秒
      "TcpKeepaliveIntvl": 5, //当TcpKeepalive探测没有确认时，重新发送探测的频度,单位秒
      "HeartbeatIvl": 1, //心跳间隔时长
      "HeartbeatTimeout": 1000, //心跳超时时长
      "HeartbeatTtl": 10000 //心跳TTL
    }
  },
  "AppSettings": {
    "HookApi": true
  },
  "ApiTest": {
    "Qps": 100,
    "HttpRouteAddress": "http://47.98.229.139",
    //"Station": "Authority",
    //"Api": "v1/login/hpc",
    //"Argument": "{\"MobilePhone\":\"15618965007\",\"UserPassword\":\"123456\"}"
    "Station": "eShop",
    "Api": "v1/shopping/productdetail",
    "Argument": "{\"ProductId\":6564467154106716160}"
  },
  "LogRecorder": {
    "console": true,
    "level": "Debug",
    /*是否开启跟踪日志*/
    "monitor": false,
    /*是否开启SQL语句日志*/
    "sql": false
  }
}